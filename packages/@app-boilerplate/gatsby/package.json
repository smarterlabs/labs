{
  "name": "@app-boilerplate/gatsby",
  "private": true,
  "scripts": {
    "build": "NODE_ENV=production npx gatsby build --verbose",
    "clean": "npx gatsby clean",
    "cms": "cp -a ../sanity/dist public/admin",
    "deploy": "yarn run deploy:staging",
    "deploy:production": "yarn run prepare-deploy && npx netlify deploy --dir=public --functions=../netlify-functions/dist --prod",
    "deploy:staging": "yarn run prepare-deploy && npx netlify deploy --dir=public --functions=../netlify-functions/dist",
    "dev": "NODE_TLS_REJECT_UNAUTHORIZED=0 npx gatsby develop --https --open",
    "develop": "yarn run dev",
    "netlify-config": "node src/utils/create-netlify-config",
    "prepare-deploy": "run-p cms netlify-config",
    "serve": "npx gatsby serve --open",
    "start": "yarn run dev"
  },
  "dependencies": {
    "@babel/runtime": "^7.6.0",
